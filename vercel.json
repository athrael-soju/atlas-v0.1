{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "/app/api/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/hello",
      "dest": "api/index.py"
    },
    {
      "src": "/app/api/*",
      "dest": "/"
    }
  ]
}
