{
  "version": 2,
  "builds": [
    {
      "src": "app/api/fast-api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "app/requirements-api.txt",
        "memory": 1024,
        "maxDuration": 60
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/fast-api/(.*)",
      "dest": "app/api/fast-api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
