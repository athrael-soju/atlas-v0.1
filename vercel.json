{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/fast-api/(.*)",
      "dest": "index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
