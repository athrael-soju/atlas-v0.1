{
  "version": 2,
  "builds": [
    {
      "src": "app/api/fast-api/index.py",
      "use": "@vercel/python",
      "config": {
        "requirementsPath": "app/requirements-api.txt",
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/fast-api/(.*)",
      "dest": "api/fast-api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
