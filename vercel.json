{
  "version": 2,
  "builds": [
    {
      "src": "app/api/fast-api//*.py",
      "use": "@vercel/python",
      "config": {
        "excludeFiles": "/{.next,*.cache,node_modules,public,app}/**"
      }
    },
    {
      "src": "next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [{ "src": "/app/api/webhook", "dest": "/app/api/webhook.py" }]
}
